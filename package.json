{
  "name": "command-interface",
  "version": "3.0.2",
  "description": "Build powerful command-line interfaces from simple ES6 modules.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "sh sh/test.sh",
    "tdd": "yarn run test -- --reporter min --watch",
